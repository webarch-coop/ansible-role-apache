Alias "/.well-known/acme-challenge" "{{ apache_document_root }}/.well-known/acme-challenge"
<Directory "{{ apache_document_root }}/.well-known/acme-challenge">
  Options None
  AllowOverride None
  ForceType text/plain
  Allow from All
  H2Upgrade off
</Directory>
